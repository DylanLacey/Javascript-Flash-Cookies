{
  "name": "flashcookies.js",
  "version": "2.0.0-pre4",
  "description": "JavaScript library and .swf for cross-domain flashcookies.\"",
  "main": "swfstore.min.js",
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "scripts": {
    "server": "python -m SimpleHTTPServer || python -m http.server",
    "test": "npm run jshint && npm run saucelabs",
    "saucelabs": "grunt test",
    "jshint": "jshint Gruntfile.js tests/spec/*.js src/*.js",
    "beautify": "js-beautify --replace Gruntfile.js tests/spec/*.js src/*.js",
    "build": "grunt build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nfriedly/Javascript-Flash-Cookies.git"
  },
  "author": "Nathan Friedly - http://nfriedly.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfriedly/Javascript-Flash-Cookies/issues"
  },
  "homepage": "https://github.com/nfriedly/Javascript-Flash-Cookies",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-connect": "^0.7.1",
    "grunt-contrib-uglify": "^0.4.0",
    "grunt-exec": "^0.4.6",
    "grunt-saucelabs": "^6.0.0",
    "js-beautify": "~1.4.2",
    "jshint": "~2.3.0"
  },
  "dependencies": {
    "grunt": "~0.4.2",
    "grunt-saucelabs": "~4.1.2"
  },
  "jshintConfig": {
    "undef": true,
    "unused": true,
    "newcap": true,
    "browser": true,
    "devel": true
  },
  "todoJshintConfig": {
    "strict": true,
    "camelcase": true
  },
  "keywords": [
    "javascript",
    "flash",
    "flashcookies",
    "swf"
  ]
}
